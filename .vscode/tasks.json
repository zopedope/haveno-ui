{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "TS Watch",
      "type": "process",
      "command": "./.vscode/tscwatch.sh",
      "isBackground": true,
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "never",
        "echo": false,
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": "$tsc-watch",
      "runOptions": {
        "runOn": "folderOpen"
      }
    }
  ]
}
